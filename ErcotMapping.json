{
        "inputs":{
                "iso": "ercot",
                "bucketname" : "s3://us-ercot/" 

        },
        "dam" : {
                "reportfolder":"00012331",
                "node":"SettlementPoint",
                "functions":{
                        "datetimeCombineErcot" : {
                                "dateCol":"DeliveryDate",
                                "hourCol" :"HourEnding",
                                "format":"%m/%d/%Y",
                                "timezone":"US/Central",
                                "hour_replace":":00",
                                "minCol":0
                                },
                        "dropCol" : ["DSTFlag"],
                        "renameCol" : {
                                "DeliveryDate" : "Date",
                                "SettlementPoint" : "node",
                                "SettlementPointPrice" : "px_da"}
                        }
        },
        "ancillary":{
                "reportfolder":"00012329",
                "functions":{
                        "datetimeCombineErcot" : {
                                "dateCol":"DeliveryDate",
                                "hourCol" :"HourEnding",
                                "format":"%m/%d/%Y",
                                "timezone":"US/Central",
                                "hour_replace":":00",
                                "minCol":0
                                },
                        "pivot" : {
                                "values" : ["MCPC"],
                                "index" : ["DeliveryDate","DSTFlag"],
                                "columns" : ["AncillaryType"]},
                        "dropCol" : ["DSTFlag"],
                        "renameCol" : {
                                "DeliveryDate" : "Date",
                                "RRS" : "px_spin",
                                "REGUP" : "px_up",
                                "REGDN" : "px_down",
                                "NSPIN" : "px_nospin"}
                        }
        },
        "rtm" : {
                "reportfolder":"00012301",
                "node":"SettlementPointName",
                "functions":{
                        "datetimeCombineErcot" :{
                                "dateCol":"DeliveryDate",
                                "hourCol" :"DeliveryHour",
                                "minCol" :"DeliveryInterval",
                                "format":"%m/%d/%Y",
                                "timezone":"US/Central",
                                "hour_replace":""
                                },
                        "dropCol" : ["DSTFlag","SettlementPointType"],
                        "renameCol" : {
                                "DeliveryDate" : "Date",
                                "SettlementPointName" : "node",
                                "SettlementPointPrice" : "px_rt"}
                        }
        },
        
        "output":{
                "functions":{
                        "resample":{
                                "freq":"H",
                                "function":"mean",
                                "timezone":"US/Central",
                                "format":"%Y-%m-%d %H:%M:%S",
                                "on": "Date"}
                        }
                
        }

}

